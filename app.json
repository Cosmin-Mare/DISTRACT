{
  "expo": {
    "name": "DISTRACT",
    "slug": "DISTRACT",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cosminmare.distract",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.cosminmare.distract",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "sounds": ["./alarm.wav"]
        }
      ], [
        "expo-splash-screen",
        {"image": "./icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "71f76894-997d-4d69-9ddb-42920a0d99bc"
      }
    },
    "owner": "cosmin_mare"
  }
}
